#include <stdio.h>
#include "list.h"
int main()
{
int N,ctr,val;
scanf("%d",&N);
for(ctr=1;ctr<=N;ctr++)
{
    scanf("%d",&val);
    if(val%2==0)
    {
        append(val);
    }
    else
    {
        prepend(val);
    }
}
    for(int index=0; index < getSize(); index++)
    {
        printf("%d ", array[index]);
    }
    return 0;
}
